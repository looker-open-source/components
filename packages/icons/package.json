{
  "name": "@looker/icons",
  "license": "MIT",
  "version": "0.7.3",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/looker-open-source/components",
    "directory": "packages/icons"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:all": "yarn clean && yarn build:icons && yarn run build:es2015 && yarn run build:esm && yarn run build:cjs",
    "build:es2015": "tsc --project tsconfig.build.json --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs",
    "build:icons": "node bin/build.js",
    "clean": "rm -Rf ./dist",
    "declaration": "tsc --project tsconfig.json",
    "develop": "yarn build:es --watch --verbose & yarn declaration --watch"
  },
  "devDependencies": {
    "@svgr/babel-plugin-add-jsx-attribute": "^4.2.0",
    "@svgr/cli": "^4.3.2",
    "@types/glob": "^7.1.1",
    "@types/node": "^12.12.7",
    "@types/rimraf": "^2.0.2",
    "glob": "^7.1.3",
    "lodash": "^4.17.15",
    "ora": "^4.0.2",
    "rimraf": "^3.0.0"
  },
  "gitHead": "05b63c793e74648ca4ada0d1bb9d4cc278256778"
}
