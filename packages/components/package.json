    "@types/react": "^16.9.19",
    "@types/react-resize-detector": "^4.2.0",
    "@types/styled-components": "^4.4.1",
    "@types/uuid": "^3.4.7",
    "csstype": "^2.6.8",
    "enzyme": "^3.11.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "styled-components": "^4.4.1"
  },
