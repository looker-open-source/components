    "d3-color": "^1.4.0",
    "d3-hsv": "^0.1.0",
    "focus-trap": "^5.1.0",
    "lodash": "^4.17.15",
    "polished": "^3.4.4",
    "popper.js": "^1.16.1",
    "react-hotkeys": "^2.0.0",
    "react-popper": "^1.3.7",
    "react-resize-detector": "^4.2.1",
    "react-transition-group": "^4.3.0",
    "styled-system": "^5.1.4",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@looker/components-test-utils": "^0.7.15",
    "@types/d3-color": "^1.2.2",
