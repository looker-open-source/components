  "main": "src/index.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@looker/components-providers": "^1.5.4",
    "@looker/design-tokens": "^2.7.0",
    "@popperjs/core": "^2.6.0",
    "@styled-icons/material": "10.34.0",
    "@styled-icons/material-outlined": "10.34.0",
    "@styled-icons/material-rounded": "10.34.0",
    "@styled-icons/styled-icon": "^10.6.3",
    "d3-color": "^2.0.0",
    "d3-hsv": "^0.1.0",
    "hotkeys-js": "^3.8.3",
    "i18next": "20.3.1",
    "react-hotkeys-hook": "2.3.1",
    "react-i18next": "11.8.15",
    "resize-observer-polyfill": "^1.5.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@looker/components-test-utils": "^1.5.4",
    "@looker/eslint-config-oss": "^1.7.3",
    "@storybook/testing-react": "^0.0.22",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.5",
    "csstype": "^3.0.8",
    "jest-styled-components": "^7.0.4",
    "lodash": "^4.17.20",
    "puppeteer": "^9.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^16.13.1",
    "styled-components": "^5.3.0"
  },
  "peerDependencies": {
    "lodash": "^4.17.20",
    "react": "^16.11",
    "react-dom": "^16.11",
    "react-is": "^16.11",
    "styled-components": "^5"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@looker/eslint-config-oss"
    ],
    "overrides": [
      {
        "files": [
          "*.tsx",
          "*.ts"
        ],
        "rules": {
          "@typescript-eslint/no-explicit-any": "off",
          "i18next/no-literal-string": "off"
        }
      },
      {
        "files": [
          "*.spec.*",
          "*.stories.*",
          "**/__mocks__/**",
          "**/stories/**"
        ],
        "rules": {
          "i18next/no-literal-string": "off",
          "jest/consistent-test-it": "off"
        }
      },
      {
        "files": [
          "*.tsx",
          "*.ts"
        ],
        "excludedFiles": [
          "*.spec.*",
          "*.stories.*",
          "**/stories/**",
          "**/__mocks__/**"
        ],
        "rules": {
          "no-restricted-imports": [
            "error",
            {
              "paths": [
                {
                  "message": "Please import icons from '@styled-icons/material/SomeIcon'",
                  "name": "@styled-icons/material"
                },
                {
                  "message": "Please import icons from '@styled-icons/material-outlined/SomeIcon'",
                  "name": "@styled-icons/material-outlined"
                },
                {
                  "message": "Please import icons from '@styled-icons/material-rounded/SomeIcon'",
                  "name": "@styled-icons/material-rounded"
                }
              ]
            }
          ]
        }
      }
    ]
  },
  "gitHead": "62febc162e3e45124f403e2c84fc741f68fe6714"
}
