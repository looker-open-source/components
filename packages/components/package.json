    "d3-color": "^1.4.1",
    "d3-hsv": "^0.1.0",
    "date-fns": "^2.12.0",
    "focus-trap": "^5.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "polished": "^3.5.1",
    "react-day-picker": "^7.3.0",
    "react-hotkeys": "^2.0.0",
    "react-resize-detector": "^4.2.1",
    "react-transition-group": "^4.3.0",
    "styled-system": "^5.1.5",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
