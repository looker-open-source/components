{
  "name": "looker-lens",
  "version": "0.0.1-beta.13",
  "main": "dist/index",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
