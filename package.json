    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.3",
    "@svgr/cli": "^4.2.0",
    "@types/d3-color": "^1.2.2",
    "@types/d3-hsv": "^0.1.3",
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/react-intl": "^2.3.17",
    "@types/react-resize-detector": "^4.0.1",
    "@types/react-test-renderer": "^16.8.1",
    "@types/react-transition-group": "^2.9.0",
    "@types/styled-system": "^3.0.9",
    "@types/uuid": "^3.4.4",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "css-loader": "^2.1.1",
    "csstype": "^2.6.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "glob": "^7.1.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "^24.7.1",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.5",
    "ora": "^3.4.0",
    "pre-commit": "1.2.2",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react-docgen-typescript": "^1.12.3",
    "react-styleguidist": "7.3.10",
    "react-test-renderer": "^16.8.6",
    "request": "^2.88.0",
    "rimraf": "^2.6.3",
    "styled-components": "^3.4.9",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "^2.2.1",
    "stylelint-processor-styled-components": "^1.6.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "8.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.3",
    "typescript-tslint-plugin": "^0.3.1",
    "typings-for-css-modules-loader": "1.7.0",
    "webpack": "^4.29.6"
  },
  "dependencies": {
    "body-scroll-lock": "^2.6.1",
    "chroma-js": "^2.0.3",
    "clean-tag": "^3.1.1",
    "d3-color": "^1.2.3",
    "d3-hsv": "^0.1.0",
    "deepmerge": "^3.2.0",
    "focus-trap-react": "^6.0.0",
    "polished": "^3.2.0",
    "popper.js": "^1.14.7",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-hotkeys": "^1.1.4",
    "react-popper": "^1.3.3",
    "react-resize-detector": "^4.1.1",
    "react-transition-group": "^2.5.1",
    "styled-system": "^3.1.6",
    "svg-inline-loader": "^0.8.0",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "@types/react": "16.8.6",
    "@types/react-dom": "16.8.6",
    "@types/styled-system": "^3.0.9",
    "styled-components": "^3.4.9"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "setupTestFrameworkScriptFile": "./test/setupJest.js",
    "transform": {
      "^.+\\.(jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)(spec|test).(ts|js)?(x)"
    ]
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "stylelint",
      "prettier --write",
      "tslint -p tsconfig.test.json --fix",
      "git add"
    ],
    "**/*.js?(x)": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ],
  "publishConfig": {
    "registry": "https://nexusrepo.looker.com/repository/looker-npm-private/"
  },
  "scripts": {
    "start": "yarn build-icons && styleguidist server",
    "build": "yarn build-components && yarn build-styleguide",
    "build-icons": "node bin/icons/build_icon_components.js",
    "build-components": "yarn clean && yarn build-icons && tsc -p tsconfig.build.json && babel src --out-dir dist --extensions \".ts,.tsx,.js,.jsx\" -s",
    "build-styleguide": "yarn clean && yarn build-icons && styleguidist build && tar -zcvf lens-styleguide.tar.gz styleguide && rimraf styleguide",
    "clean": "rimraf dist && rimraf styleguide && rimraf src/icons/build && rimraf lens-styleguide.tar.gz",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:css": "stylelint \"src/**/*.ts?(x)\"",
    "lint:ts": "tslint -p tsconfig.test.json --format stylish",
    "lint-staged": "$(yarn bin)/lint-staged",
    "npmrc": "bin/create_npmrc.sh",
    "npmrc:clean": "rm .npmrc",
    "prepublishOnly": "yarn clean && yarn && yarn test && yarn build-components",
    "prettify": "yarn prettier \"./**/*.ts?(x)\" \"./**/*.js?(x)\" '!./node_modules/**' '!./dist/**' --write",
    "release": "npm publish",
    "test": "yarn build-icons && tsc -p tsconfig.test.json && yarn jest"
  },
  "types": "dist"
}
