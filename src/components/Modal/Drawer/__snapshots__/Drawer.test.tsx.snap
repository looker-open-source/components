  background: #F3F2FF;
  border-color: #F3F2FF;
  color: #6C43E0;
}

.c18:active,
.c18.active {
  background: #E8E5FF;
  border-color: #E8E5FF;
  color: #6C43E0;
}

.c8 {
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  text-transform: none;
}

.c15 {
  border-top: 1px solid transparent;
}

.c15.overflow {
  border-top-color: #DEE1E5;
  box-shadow: inset 0 -16px 16px -16px #DEE1E5;
}

<div
    className="c0"
  >
    <div
      className="faux c1 c2"
      onClick={[Function]}
    />
    <div
      className="faux c3 c4 c5"
    >
      <header
        className="c6"
      >
        <div
          className="c7"
        >
          <h2
            className="c8 c9"
          >
            Pick stuff
          </h2>
        </div>
        <div
          className="c10"
        >
          <button
            className="c11 c12"
            onClick={[Function]}
            style={
              Object {
                "lineHeight": 1,
                "padding": 0,
              }
            }
          >
            <div
              className="c13 c14"
            >
              <svg
                fill="none"
                height="100%"
                viewBox="0 0 24 24"
                width="100%"
              >
                <path
                  d="M19 6.41L17.59 5 12 10.59 6.41 5 5 6.41 10.59 12 5 17.59 6.41 19 12 13.41 17.59 19 19 17.59 13.41 12 19 6.41z"
                  fill="currentColor"
                />
              </svg>
            </div>
          </button>
        </div>
      </header>
      <div
        style={
          Object {
            "display": "none",
            "height": 0,
            "position": "absolute",
            "visibility": "hidden",
            "width": 0,
          }
        }
      />
      <div
        className="undefined false c15 c16"
        overflow="scroll"
      >
        Stuff and things...
      </div>
      <footer
        className="c17"
      >
        <button
          className="c18 c19"
        >
          Cancel
        </button>
        <button
          className="c20 c21"
        >
          Pick some stuff
        </button>
      </footer>
    </div>
  </div>,
]
`;
