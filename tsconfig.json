{
  "extends": "@looker/eslint-config/tsconfig.react",
  "compilerOptions": {
    "paths": {
      "@looker/*": ["packages/*/src"],
      "*": ["node_modules", "packages"]
    },
    "plugins": [
      {
        "transform": "typescript-plugin-styled-components",
        "type": "config"
      }
    ]
  },
  "include": ["packages"],
  "exclude": ["dist"]
}
