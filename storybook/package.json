{
  "name": "storybook",
  "version": "1.4.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "develop": "start-storybook  -c .storybook -p 3333  --host 0.0.0.0",
    "build": "build-storybook -c .storybook",
    "storyshots-build": "export storybookBuildMode=fast && yarn build",
    "storyshots-preview-backup": "mv .storybook/preview.js .storybook/preview.js.bu",
    "storyshots-preview-restore": "mv .storybook/preview.js.bu .storybook/preview.js",
    "prestoryshots-task": "yarn storyshots-build && yarn storyshots-preview-backup",
    "storyshots-task": "yarn jest",
    "poststoryshots-task": "yarn storyshots-preview-restore",
    "snapshots": "yarn storyshots-task --config ./jest-image-snapshots.config.js",
    "snapshots-update": "yarn snapshots -u",
    "a11y": "yarn storyshots-task --config ./jest-a11y.config.js"
  },
  "dependencies": {
    "@looker/components": "^1.4.0",
    "@looker/components-providers": "^1.3.1",
    "@looker/components-theme-editor": "^1.4.0",
    "@looker/design-tokens": "^1.3.1",
    "@storybook/react": "^6.2.9",
    "lodash": "^4.17.20",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^16.13.1",
    "storybook": "^6.2.9",
    "styled-components": "5.3.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-storyshots": "^6.2.9",
    "@storybook/addon-storyshots-puppeteer": "^6.2.9",
    "babel-loader-exclude-node-modules-except": "^1.1.2",
    "tsconfig-paths-webpack-plugin": "^3.5.1"
  }
}
